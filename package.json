{
  "name": "rs-react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "serve": "NODE_ENV=production ts-node-esm server.ts",
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "prettier": "prettier --check .",
    "lint": "eslint src",
    "format": "prettier --write .",
    "cypress:open": "cypress open",
    "cypress:coverage": "nyc cypress run && nyc report --reporter=text-lcov > coverage.lcov && cypress-istanbul-reporter",
    "cypress:serve:coverage": "npm run build && start-server-and-test serve http-get://localhost:3001 cypress:coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "ejs": "^3.1.9",
    "ejs-mate": "^4.0.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "autoprefixer": "^10.4.14",
    "babel-plugin-istanbul": "^6.1.1",
    "compression": "^1.7.4",
    "cypress": "^12.11.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "serve-static": "^1.15.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.3.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript"
  }
}
